import java.util.Scanner;
public class Main
{
	public static void main(String[] args) 
	{
	    float num;
	    float num1;
	    float num2;
	    float num3;
	    float num4;
	    float percentage;
	    
	    Scanner sc=new Scanner(System.in);
		System.out.println("Enter the marks of Mathsmatics:");
		num=sc.nextFloat();
		
		System.out.println("Enter the marks of English:");
		num1=sc.nextFloat();
		
		System.out.println("Enter the marks of Social Studies:");
		num2=sc.nextFloat();
		
		System.out.println("Enter the marks of Hindi:");
		num3=sc.nextFloat();
		
		System.out.println("Enter the marks of Science:");
		num4=sc.nextFloat();
		
	/*	System.out.println(num);
		System.out.println(num1);
		System.out.println(num2);
		System.out.println(num3);
		System.out.println(num4); */
		
		
		percentage=(num+num1+num2+num3+num4)/5;
		
		if(percentage>=90)
		{
		    System.out.println("You have scored O+.");
		}
		else if(percentage>=70 && percentage<90)
		{
		    System.out.println("You have scored A grade.");
		}
		else if(percentage>=50 && percentage<70)
		{
		    System.out.println("You have scored B grade.");
		}
		else
		{
		    System.out.println("You are failed.Work hard dear!!");
		}
		
	}
}
